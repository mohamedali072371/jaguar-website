{
  "hosting": {
    "site": "shameemsha",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/new-year-greetings",
        "destination": "/new-year-greetings/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.reality",
        "headers": [
          {
            "key": "Content-Type",
            "value": "model/vnd.reality"
          }
        ]
      }
    ]
  }
}
