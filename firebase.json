{
  "hosting": {
    "site": "jaguar-promotion",
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      },
      {
        "source": "/3d-envinorment",
        "destination": "/3d-envinorment/index.html"
      },
      {
        "source": "/ar",
        "destination": "/ar/index.html"
      }
    ],
    "headers": [
      {
        "source": "**/*.reality",
        "headers": [
          {
            "key": "Content-Type",
            "value": "model/vnd.reality"
          }
        ]
      }
    ]
  }
}
